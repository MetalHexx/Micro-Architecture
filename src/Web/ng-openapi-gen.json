{
  "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:5000/swagger/v1/swagger.json",
  "output": "src/app/gateway-api",
  "ignoreUnusedModels": false,
  "minParamsForContainer": 10,
  "sortParams": "desc",
  "defaultTag": "Api",
  "module": "GatewayApiModule",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": false,
  "generateExamples": false,
  "camelCase": false
}